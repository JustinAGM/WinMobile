    <!--Info Section Start-->
    <div class="info-container" id="info">
      <section class="box-container">
        <div class="box">
          <img src="/images/calendar.png" alt="dailyquest" />
          <h3>Daily Quests</h3>
        </div>
        <div class="box">
          <img src="/images/elements.jpg" alt="elements" />
          <h3>Elemental Classes</h3>
        </div>
        <div class="box">
          <img src="/images/reward.png" alt="rewards" />
          <h3>Generous Rewards</h3>
        </div>
        <div class="box">
          <img src="/images/money.jpg" alt="cash" />
          <h3>In-game Purchases</h3>
        </div>
        <div class="box">
          <img src="/images/mobile.png" alt="mobile" />
          <h3>Mobile Gaming hours</h3>
        </div>
        <div class="box">
          <img src="/images/brain.png" alt="mind" />
          <h3>Mind Games</h3>
        </div>
      </section>
    </div>
    <!--Info Section End-->
    <!--Contact Section Start-->
    <section class="contact" id="contact">
      <h1 class="heading"><span>contact</span></h1>
      <div class="box-container">
        <!-- Box 1: Email -->
        <div
          class="box center"
          style="background: url('/images/background3.jpg') no-repeat"
        >
          <img src="/images/email.jpg" alt="Email Icon" />
          <h3>Email</h3>
          <a href="mailto:justin.a.g.machica@gmail.com">Creator Email</a>
          <a href="mailto:gameemail@example.com">Game Email</a>
        </div>

        <!-- Box 2: Social -->
        <div
          class="box center"
          style="background: url('/images/background3.jpg') no-repeat"
        >
          <img src="/images/@.jpg" alt="Social Media Icon" />
          <h3>Social</h3>
          <a
            href="https://www.linkedin.com/in/machica-justin-alexis-603313244"
            target="_blank"
            rel="noopener noreferrer"
          >
            Creator's LinkedIn
          </a>
        </div>

        <!-- Box 3: Address -->
        <div
          class="box center"
          style="background: url('/images/background3.jpg') no-repeat"
        >
          <img src="/images/location.jpg" alt="Location Icon" />
          <h3>Address</h3>
          <a
            href="https://www.google.com/maps?q=1234+Test+Street,+Coding+Village,+Philippines+1081"
            target="_blank"
            rel="noopener noreferrer"
          >
            #1234 Test Street, Coding Village, Philippines 1081
          </a>
        </div>
      </div>

      <!-- Contact Form -->
      <form action="/submit" method="POST" class="form">
        <h1 class="heading"><span>get</span> in touch</h1>
        <div class="flex">
          <!-- Name Input -->
          <div>
            <label for="name">Your Name</label>
            <input
              type="text"
              id="name"
              placeholder="Your name"
              class="input"
              required
            />
          </div>

          <!-- Email Input -->
          <div>
            <label for="email">Your Email</label>
            <input
              type="email"
              id="email"
              placeholder="Your email"
              class="input"
              required
            />
          </div>

          <!-- Phone Number Input -->
          <div>
            <label for="number">Your Number</label>
            <input
              type="number"
              id="number"
              placeholder="Your number"
              class="input"
              required
            />
          </div>

          <!-- Country Input -->
          <div>
            <label for="country">Your Country</label>
            <input
              type="text"
              id="country"
              placeholder="Your country"
              class="input"
              required
            />
          </div>
        </div>

        <!-- Message Textarea -->
        <div>
          <label for="message">Your Message</label>
          <textarea
            id="message"
            name="message"
            class="input"
            placeholder="Your message"
            required
          ></textarea>
        </div>

        <!-- Submit Button -->
        <input type="submit" value="Send Message" class="btn" />
      </form>
    </section>
    <!--Contact Section End-->
    <!-- Footer Section Start -->
    <footer class="footer">
      <div class="footer-container">
        <div class="footer-links">
          <h4>Quick Links</h4>
          <ul>
            <li><a href="#home">Home</a></li>
            <li><a href="#features">Features</a></li>
